  \verb 10.1111/eci.12376
  \endverb
  \field{issn}{1365-2362}
  \field{issue}{2}
  \field{pages}{204\bibrangedash 214}
  \field{title}{Transparent reporting of a multivariable prediction model for
  individual prognosis or diagnosis (TRIPOD): the TRIPOD Statement.}
  \field{volume}{45}
  \verb{file}
  \verb :PDFs/collins2015.pdf:PDF
  \endverb
  \field{journaltitle}{European journal of clinical investigation}
  \field{month}{02}
  \field{year}{2015}
\endentry

